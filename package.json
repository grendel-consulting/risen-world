{
  "name": "risen-world",
  "version": "0.1.10",
  "description": "Strategic narrative toy, mashing up some Roguelike, 4X and CRPG concepts",
  "main": "index.js",
  "repository": "https://github.com/grendel-consulting/risen-world.git",
  "author": "James Ramirez <ramirezj@users.noreply.github.com>",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": {
    "production": [
      "last 1 versions",
      "not dead",
      ">5%"
    ],
    "ssr": [
      "node 14",
      "node 15"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@storybook/addon-actions": "6.1.21",
    "@storybook/addon-essentials": "6.2.0",
    "@storybook/addon-links": "6.1.21",
    "@storybook/react": "6.2.0",
    "@types/jest": "26.0.23",
    "@types/node": "14.14.45",
    "@types/react": "17.0.5",
    "@types/react-dom": "17.0.5",
    "@types/react-test-renderer": "17.0.1",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "alex": "9.1.0",
    "babel-loader": "8.2.2",
    "babel-plugin-remove-graphql-queries": "3.5.0",
    "babel-plugin-root-import": "6.6.0",
    "babel-preset-gatsby": "1.1.0",
    "babel-preset-react-app": "10.0.0",
    "commitizen": "4.2.4",
    "cypress": "7.2.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.25.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-root-import": "1.0.4",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "6.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "jest-it-up": "1.2.3",
    "lint-staged": "10.5.4",
    "markdownlint-cli": "0.27.1",
    "prettier": "2.2.1",
    "react-test-renderer": "17.0.2",
    "standard-version": "9.1.1",
    "start-server-and-test": "1.12.1",
    "ts-jest": "26.5.4",
    "typescript": "4.2.3"
  },
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc",
    "lint": "yarn run lint:md && yarn run lint:ts && yarn run lint:lang",
    "lint:md": "markdownlint \"**/*.+(md|mdx)\"",
    "lint:ts": "eslint --ignore-path .gitignore \"**/*.+(ts|js|tsx)\"",
    "lint:lang": "alex",
    "format": "yarn run format:md && yarn run format:json && yarn run format:ts && yarn run format:yaml",
    "format:md": "prettier \"**/*.+(md|mdx)\" --write",
    "format:json": "prettier \"**/*.+(json)\" --write",
    "format:ts": "prettier \"**/*.+(jsjsx|ts|tsx)\" --write",
    "format:yaml": "prettier \"**/*.+(yml|yaml)\" --write",
    "release": "standard-version",
    "test": "jest",
    "test:coverage": "jest --coverage --watchAll=false",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "start-server-and-test start http://localhost:8000 cypress:open",
    "test:e2e:ci": "start-server-and-test start http://localhost:8000 cypress:run",
    "posttest": "jest-it-up",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "renovate": {
    "extends": [
      "config:base",
      ":pinAllExceptPeerDependencies",
      "schedule:weekly"
    ],
    "labels": [
      "dependencies"
    ],
    "packageRules": [
      {
        "depTypeList": [
          "dependencies",
          "peerDependencies"
        ],
        "automerge": true,
        "major": {
          "automerge": false
        }
      },
      {
        "depTypeList": [
          "devDependencies"
        ],
        "automerge": true
      }
    ],
    "postUpdateOptions": [
      "yarnDedupeHighest"
    ],
    "timezone": "Europe/London",
    "vulnerabilityAlerts": {
      "labels": [
        "security"
      ],
      "assignees": [
        "@ramirezj"
      ]
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write"
    ],
    "*.{css}": "prettier --write",
    "*.{md,mdx}": [
      "markdownlint",
      "prettier --write"
    ],
    "*.{json,yml,yaml}": "prettier --write"
  },
  "dependencies": {
    "@babel/core": "7.13.10",
    "@mdx-js/mdx": "1.6.22",
    "@mdx-js/react": "1.6.22",
    "@styled-system/css": "5.1.5",
    "babel-jest": "26.6.3",
    "gatsby": "3.1.1",
    "gatsby-plugin-mdx": "2.1.0",
    "gatsby-plugin-page-creator": "3.1.0",
    "gatsby-plugin-sharp": "3.1.1",
    "gatsby-plugin-theme-ui": "0.6.0",
    "gatsby-remark-images": "4.1.0",
    "gatsby-source-filesystem": "3.1.0",
    "gatsby-transformer-sharp": "3.1.0",
    "mkdirp": "1.0.4",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "theme-ui": "0.6.0",
    "tsconfig-paths-webpack-plugin": "3.5.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
